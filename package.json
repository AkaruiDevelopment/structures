{
  "name": "@akarui/structures",
  "author": {
    "name": "AkaruiDevelopment"
  },
  "maintainers": ["USERSATOSHI"],
  "version": "0.0.1",
  "repository": {
    "type": "github",
    "url": "https://github.com/USERSATOSHI/Structures"
  },
  "readme": "./README.md",
  "bugs": {
    "url": "https://github.com/USERSATOSHI/Structures/issues"
  },
  "description": "Abstract Data Structures That Make Storing data Much easy.",
  "license": "MIT",
  "devDependencies": {
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "main": "./dist/cjs/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "test": "node tests",
    "start": "node index.js",
    "build": "npm run addpkg && npx tsc -p tsconfig.esm.json && npx tsc -p tsconfig.cjs.json && npm run createpack",
    "createpack": "node packers/pack.js",
    "addpkg": "node packers/addpkgjson.js",
    "bundleTsFiles": "node packers/bundle.js",
    "lint": "eslint . --ext .ts --fix --ext .js",
    "postinstall": "npm run build"
  }
